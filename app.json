{
  "expo": {
    "jsEngine": "hermes",
    "name": "PiCK",
    "slug": "pick",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/Logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/images/Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "@react-native-firebase/app",
      [
        "expo-font",
        {
          "fonts": [
            "./app/assets/font/Medium.ttf",
            "./app/assets/font/Regular.ttf",
            "./app/assets/font/SemiBold.ttf"
          ]
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "프로필 사진 변경 및 버그 제보 이미지 업로드에 사용됩니다."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "agpVersion": "8.4.0",
            "ndkVersion": "26.1.10909125"
          }
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/Logo.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "versionCode": 47
    },
    "extra": {
      "eas": {
        "projectId": "d2da24b2-10fc-44da-8e05-35dfa3b0155b"
      }
    },
    "owner": "b20f920a",
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
